    <PLATFORM_FAMILY>x86</PLATFORM_FAMILY>
    <CustomFilter>
    </CustomFilter>
    <INSTRUCTION_SET Condition="'$(INSTRUCTION_SET)'==''">x86</INSTRUCTION_SET>
