    <HFiles Include="$(SPOCLIENT)\devicecode\Targets\OS\Win32\lwip_1_4_1_os\perf.h" />
    <HFiles Include="$(SPOCLIENT)\devicecode\Targets\OS\Win32\lwip_1_4_1_os\sys_arch.h" />
  </ItemGroup> 
